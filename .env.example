# Supabase Configuration - Frontend (Public)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Supabase Configuration - Server (Private)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key
PORT=3005

# Admin Configuration
VITE_ADMIN_SECRET_KEY=your_secure_admin_password_here

# SMTP Email Configuration (Server)
# Required for sending email on approve/reject wallet approval
SMTP_HOST=smtp.yourprovider.com
SMTP_PORT=587
SMTP_USER=your_smtp_username
SMTP_PASS=your_smtp_password
SMTP_SECURE=false
SMTP_FROM="EvenOddPro <no-reply@evenoddpro.com>"

# Optional: Direct PostgreSQL connection for scripts (if used)
DB_HOST=your_db_host
DB_PORT=5432
DB_USERNAME=your_db_username
DB_PASSWORD=your_db_password
DB_NAME=postgres